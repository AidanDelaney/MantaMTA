<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="SqlServer" connectionString="Data Source=.\;Initial Catalog=MANTA_MTA;Integrated Security=True" />
  </connectionStrings>
  <system.diagnostics>
    <trace useGlobalLock="false" autoflush="true" indentsize="0">
      <listeners>
        <add name="LogFile" type="System.Diagnostics.TextWriterTraceListener" initializeData="MantaMtaReset.log"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>